<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
@{
    ViewData["Title"] = "EditMyClientsLogos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>EditMyClientsLogos</h1>

@model MyClientsLogosEntity
<form method="post">
    <div class="container">
        <div>
            <label>Text</label>
            <input type="text" class="form-control" asp-for="ImageUrlLogo" 
            >

        </div>
        <div>
            <label>Href</label>
            <input type="text" class="form-control" asp-for="Href" placeholder="Text">

        </div>
      
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>