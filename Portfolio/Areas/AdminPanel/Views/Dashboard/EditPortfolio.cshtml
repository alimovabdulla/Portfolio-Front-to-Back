<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
@{
    ViewData["Title"] = "EditPortfolio";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model PortfolioProject
<h1>EditPortfolio</h1>

<form method="post">
    <div class="container">
        <div>
            <label>Image</label>
            <input type="text" class="form-control" asp-for="ImageUrl" 
            
            
            
            >

        </div>
        <div>
            <label>Text</label>
            <input type="text" class="form-control" asp-for="ImageText" placeholder="Text">

        </div>
        <div>
            <label>Href</label>
            <input type="text" class="form-control" asp-for="Href" placeholder="Text">

        </div>
        <div>
            <label>Category</label>
            <input type="text" class="form-control" asp-for="Category" placeholder="Text">

        </div>
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>