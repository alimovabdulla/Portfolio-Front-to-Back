<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
@{
    ViewData["Title"] = "CreateTestimonial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>CreateTestimonial</h1>


 
@model TestimonialEntity
 

<form method="post">
    <div class="container">
        <div>
            <label>Text</label>
            <input type="text" class="form-control" asp-for="Text" 
            
            
            >

        </div>
        <div>
            <label>PP</label>
            <input type="text" class="form-control" asp-for="PPurl" placeholder="Text">

        </div>
        <div>
            <label>Profession</label>
            <input type="text" class="form-control" asp-for="Profession" placeholder="Text">

        </div>
        <div>
            <label>Name</label>
            <input type="text" class="form-control" asp-for="Name" placeholder="Text">

        </div>
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>