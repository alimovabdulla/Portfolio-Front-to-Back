<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
@{
    ViewData["Title"] = "EditEducation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model EducationEntity
<h1>EditEducation</h1>


<form method="post">
    <div class="container">
        <div>
            <label>University</label>
            <input type="text" class="form-control" asp-for="University" 
            
            
            >

        </div>
        <div>
            <label>Degree</label>
            <input type="text" class="form-control" asp-for="Degree" placeholder="Text">

        </div>
        <div>
            <label>Years</label>
            <input type="text" class="form-control" asp-for="Years" placeholder="Text">

        </div>

        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>