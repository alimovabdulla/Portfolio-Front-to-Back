<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
@{
    ViewData["Title"] = "EditConnectEntity";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model ConnectEntity
<h1>EditConnectEntity</h1>


<form method="post">
    <div class="container">
        <div>
            <label>Address</label>
            <input type="text" class="form-control" asp-for="Address" 
            
            
            >

        </div>
        <div>
            <label>Email</label>
            <input type="text" class="form-control" asp-for="Email" placeholder="Text">

        </div>
        <div>
            <label>Number</label>
            <input type="text" class="form-control" asp-for="Number" placeholder="Text">

        </div>

        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>